/**
* remove.du
*
* Testing the set.add() method
*
* .add() mutates the set to add a value
*/

var mySet = set();

mySet.add("dictu");
assert(mySet.contains("dictu"));
assert(mySet.len() == 1);

mySet.add("is");
assert(mySet.contains("is"));
assert(mySet.len() == 2);

mySet.add("great!");
assert(mySet.contains("great!"));
assert(mySet.len() == 3);
