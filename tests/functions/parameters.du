/**
* parameters.du
*
* Testing the parameters of a function
*/

def test(a) {
    assert(a == 10);
}

test(10);

def test1(a, b) {
    assert(a + b == 10);
}

test1(5, 5);

def test2(a, b, c) {
    assert(a == 10);
    assert(b == "hello");
    assert(c == true);
}

test2(10, "hello", true);


def optional(a?, b?) {
    assert(a == nil);
    assert(b == nil);
}

optional();

def oneOptional(a, b?) {
    assert(a == 10);
    assert(b == nil);
}

oneOptional(10);

def twoOptional(a?, b?) {
    assert(a == 10);
    assert(b == 20);
}

twoOptional(10, 20);

def threeOptional(a?, b?, c?) {
    assert(a == 10);
    assert(b == 20);
    assert(c == nil);
}

threeOptional(10, 20);
threeOptional(10, 20, nil);