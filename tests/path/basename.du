/**
 * basename.du
 *
 * Testing Path.basename()
 *
 * Returns the basename of string.
 */
import Path;

assert(Path.basename ("") == "");
assert(Path.basename ("/") == "/");
assert(Path.basename ("//a") == "a");
assert(Path.basename ("//") == "/");
assert(Path.basename ("/////") == "/");
assert(Path.basename ("/root/t.ext") == "t.ext");
