/**
 * dirname.du
 *
 * Testing Path.dirname()
 *
 * Returns the directory name of string.
 */
import Path;

assert(Path.dirname("/") == "/");
assert(Path.dirname("//") == "/");
assert(Path.dirname("") == ".");
assert(Path.dirname("a") == ".");
assert(Path.dirname("/root") == "/");
assert(Path.dirname("///root") == "/");
assert(Path.dirname("/root///a") == "/root");
